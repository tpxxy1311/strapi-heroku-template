{
  "kind": "collectionType",
  "collectionName": "movies",
  "info": {
    "singularName": "movie",
    "pluralName": "movies",
    "displayName": "movie"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "genre": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "release": {
      "type": "string"
    },
    "regisseurShort": {
      "type": "string"
    },
    "poster": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "slug": {
      "type": "string"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "movie"
    },
    "regisseur": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::regisseur.regisseur",
      "inversedBy": "movies"
    },
    "watchlists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::watchlist.watchlist",
      "mappedBy": "movie"
    },
    "reclists": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::reclist.reclist",
      "mappedBy": "movie"
    }
  }
}
